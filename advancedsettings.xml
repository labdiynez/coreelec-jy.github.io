<advancedsettings>
  <musiclibrary>
    <backgroundupdate>true</backgroundupdate>
  </musiclibrary>
  <videolibrary>
    <cleanonupdate>false</cleanonupdate>
  </videolibrary>
  <video>
    <ignorepercentatend>10</ignorepercentatend>
    <playcountminimumpercent>90</playcountminimumpercent>
    <ignoresecondsatstart>60</ignoresecondsatstart>
  </video>
  <splash>false</splash>
  <cache>
    <buffermode>1</buffermode>
    <memorysize>524288000</memorysize>
    <readfactor>50</readfactor>
  </cache>
  <network>
    <curlclienttimeout>600</curlclienttimeout>
    <curllowspeedtime>1200</curllowspeedtime>
    <curlretries>2</curlretries>
    <nfstimeout>0</nfstimeout>
    <curlkeepaliveinterval>30</curlkeepaliveinterval>
  </network>
</advancedsettings>
